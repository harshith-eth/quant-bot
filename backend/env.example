# QUANTUM DEGEN TRADING AI SWARM - ENVIRONMENT VARIABLES
# Copy this file to .env and fill in your actual values
#
# WARNING: This bot executes REAL trades with REAL money
# NEVER share your private keys or API keys with anyone

# Blockchain RPC URLs (Free public endpoints)
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_RPC_BACKUP=https://solana-api.projectserum.com
ETHEREUM_RPC_URL=https://cloudflare-eth.com

# DEX and Market Data Sources (Free/Public endpoints)
COINGECKO_API_URL=https://api.coingecko.com/api/v3
DEXSCREENER_API_URL=https://api.dexscreener.com/latest
JUPITER_API_URL=https://price.jup.ag/v4
BIRDEYE_API_URL=https://public-api.birdeye.so

# Social Media APIs (Optional - Free tiers)
TWITTER_BEARER_TOKEN=your_twitter_bearer_token_free_tier
REDDIT_CLIENT_ID=your_reddit_client_id
REDDIT_CLIENT_SECRET=your_reddit_client_secret

# Trading Settings
INITIAL_BALANCE=25.0
MIN_POSITION_SIZE=1.0
MAX_POSITION_SIZE=5.0  # Maximum SOL per position
DEFAULT_STOP_LOSS=0.30  # 30% stop loss
DEFAULT_TAKE_PROFIT=1.00  # 100% take profit
MAX_POSITIONS=5
DEFAULT_SLIPPAGE=50  # 0.5% slippage in basis points
JUPITER_PRIORITY_FEE=1000000  # 0.001 SOL priority fee

# Risk Management
MAX_DAILY_LOSS=0.50
MIN_LIQUIDITY=5000.0
MAX_MARKET_CAP=200000.0
WHALE_THRESHOLD=500.0

# Data Update Intervals (seconds)
PRICE_UPDATE_INTERVAL=5
WHALE_SCAN_INTERVAL=30
TOKEN_SCAN_INTERVAL=60
PORTFOLIO_UPDATE_INTERVAL=10

# WebSocket and Server
WEBSOCKET_HOST=localhost
WEBSOCKET_PORT=8000
DEBUG_MODE=true

# API Keys
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key

# Exchange APIs  
BINANCE_API_KEY=your_binance_api_key
BINANCE_SECRET_KEY=your_binance_secret_key

# Wallet Settings - CRITICAL FOR REAL TRADING
# WARNING: Never share your private key with anyone
# Format: Base58 encoded private key
WALLET_PRIVATE_KEY=your_solana_wallet_private_key
AUTO_TRADING_ENABLED=false

# Social Media APIs
TELEGRAM_BOT_TOKEN=your_telegram_bot_token

# Database
DATABASE_URL=postgresql://user:pass@localhost/quant_db
REDIS_URL=redis://localhost:6379 